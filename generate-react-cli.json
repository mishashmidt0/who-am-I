{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": ".ts",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true,
      "withStyles": true,
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "styles": "templates/default/styles.ts",
        "index": "templates/default/index.ts"
      }
    },
    "container": {
      "path": "src/containers",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true,
      "withStyles": true,
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "styles": "templates/default/styles.ts",
        "index": "templates/default/index.ts"
      }
    }
  }
}
